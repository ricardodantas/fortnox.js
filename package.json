{
  "name": "fortnoxab-api-js-client",
  "version": "1.0.0",
  "description": "A Fortnox AB API client wrapper. ",
  "main": "lib/fortnoxab-api-js-client.js",
  "scripts": {
    "build": "rm -fr ./lib && webpack",
    "lint": "eslint .",
    "test": "jest",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": "git+ssh://git@github.com/ricardodantas/fortnoxab-api-js-client.git",
  "keywords": [
    "fotnox",
    "invoice",
    "finances"
  ],
  "files": [
    "lib"
  ],
  "author": "Ricardo Dantas Goncalves",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ricardodantas/fortnoxab-api-js-client/issues"
  },
  "homepage": "https://github.com/ricardodantas/fortnoxab-api-js-client#readme",
  "dependencies": {
    "axios": "^0.18.1",
    "dotenv": "^6.2.0",
    "snyk": "^1.438.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "7",
    "babel-preset-minify": "^0.5.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "jest": "^23.6.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  },
  "eslintIgnore": [
    "coverage/",
    "lib/",
    "node_modules/"
  ],
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testPathIgnorePatterns": [
      "node_modules/",
      "__mocks__",
      "__mockData__"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ]
  },
  "snyk": true
}
